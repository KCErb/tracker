<div id="shared-modal">
</div>

<div id="create-tag-modal">
</div>

<div class="container">
  <h2 class="page-title">Tracker</h2>
  <%= render partial: "navbar" %>
  <div id="households-table-div">


    <div class="progress" style="margin: 50px; margin-bottom:10px;">
      <div class="progress-bar progress-bar-striped active" id='progress-bar' role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100"
      style="width: 0%;">
        <span class="sr-only">Loading Table</span>
      </div>
    </div>

    <div style="text-align: center"><h4 id='progress-message' >Loading Table</h4></div>
  </div>
  <div id="empty-table" style='display:none;'>
    <div style="text-align: center"><h4>Sorry, no households match your search.</h4></div>
  </div>
</div>
